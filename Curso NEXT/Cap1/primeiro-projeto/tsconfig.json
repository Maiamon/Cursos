{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "pages/basicos/lista2.ssx",
    "pages/basicos/jsx3.jsx",
    "pages/basicos/jsx4.jsx",
    "pages/basicos/usandotitulo.jsx",
    "components/Titulo.jsx"
  ],
  "exclude": [
    "node_modules"
  ]
}